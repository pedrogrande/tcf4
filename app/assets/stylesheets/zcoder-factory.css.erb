body {
	font-size: 18px;
	font-family: 'Play', sans-serif; /* , Arimo, Cabin, 'Dosis', Helvetica, Arial */
	line-height: 1.5em;
	color: #0070a6;
	-webkit-font-smoothing: antialiased !important;	
}
.logo {
	height: 100px;
}
.signin {
	background:#f8f8f8 url(asset-path 'mac.jpg') fixed;
	background-position: cover cover;
}
.all-caps {
	text-transform: uppercase;
	font-weight: bold;
}
.btn {
	border-radius: 0;
	border-bottom: 2px rgba(0,0,0,0.3) solid;
}
.btn-xl {
	padding: 10px 16px;
	font-size: 24px;
	line-height: 1.33;
	border-radius: 0px;
}
.label {
	border-radius: 0;
}
.labels {
	font-size: 16px;
}
.form-control {
	display: block;
	width: 100%;
	height: 60px;
	padding: 6px 12px;
	font-size: 18px;
	line-height: 1.3;
	color: #00AEFF;
	background-color: transparent;
	background-image: none;
	border: 2px solid #DDF;
	-webkit-border-radius: 0px !important;
	-moz-border-radius: 0px !important;
	border-radius: 0px !important;
	-webkit-box-shadow: inset 0 1px 1px rgba(0,174,255, 0.2);
	box-shadow: inset 0 1px 1px rgba(0,174,255, 0.2);
	-webkit-transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
	transition: border-color ease-in-out 0.15s, box-shadow ease-in-out 0.15s;
}
/** Forms **/
textarea,
input[type="text"],
input[type="password"],
input[type="datetime"],
input[type="datetime-local"],
input[type="date"],
input[type="month"],
input[type="time"],
input[type="week"],
input[type="number"],
input[type="email"],
input[type="url"],
input[type="search"],
input[type="tel"],
input[type="color"],
.uneditable-input {
	background-color: transparent;
	border-bottom: 2px solid #DDF;

	box-shadow:0 0 0px #e3e3e3 inset;
	-webkit-shadow:0 0 0px #e3e3e3 inset;
	-moz-shadow:0 0 0px #e3e3e3 inset;
	-webkit-transition: border linear .2s, box-shadow linear .2s;
	-moz-transition: border linear .2s, box-shadow linear .2s;
	-o-transition: border linear .2s, box-shadow linear .2s;
	transition: border linear .2s, box-shadow linear .2s;
	margin-bottom:20px;
	border-top:0;
	border-right: 0;
	border-left: 0;
}
select {
	-webkit-border-radius: 0px !important;
	-moz-border-radius: 0px !important;
	border-radius: 0px !important;
}
.panel {
	margin-bottom: 20px;
	background-color: #fff;
	border: 0px solid;
	border-radius: 0px;
	-webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, .0);
	box-shadow: 0 1px 1px rgba(0, 0, 0, .0);
}
.panel-heading {
	border-top-left-radius: 0px;
